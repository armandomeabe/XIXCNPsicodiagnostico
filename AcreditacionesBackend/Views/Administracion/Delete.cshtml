@model AcreditacionesBackend.Models.Accreditation

@{
    ViewBag.Title = "Eliminar Acreditación";
}

<h2>Eliminar Acreditación</h2>

<h3>¿Seguro que desea eliminar ésta acreditación?</h3>

<div>
    <h4>Acreditación</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
