        <dd>
            @Html.DisplayFor(model => model.EleccionDePago)
        </dd>

        <dt>
            Acreditación Realizada
        </dt>

        <dd>
            @Html.DisplayFor(model => model.AcreditacionRealizada)
        </dd>

        <dt>
